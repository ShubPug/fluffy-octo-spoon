import tkinter as tk
import webbrowser

def open_url():
    # Function to open the URL in the default web browser
    webbrowser.open_new("https://www.example.com")  # Replace with desired URL

# Create a Tkinter window
root = tk.Tk()
root.title("URL Button Example")

# Create a button
button = tk.Button(root, text="Open URL", command=open_url)
button.pack()

root.mainloop()
